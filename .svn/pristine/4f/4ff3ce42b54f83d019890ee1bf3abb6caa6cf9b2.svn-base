[package]
name = "core-std-lib"
version = "2.0.2"
edition = "2021"

[lib]
name = "core_std_lib"
crate-type = ["staticlib", "cdylib", "rlib"]

[dependencies]
async-trait = "0.1.88"
core-macros = { path = "../core-macros/" }
hashable = "0.0.0"
ordered-float = { version = "5.0.0", features = ["serde"] }
rstest = "0.25.0"
serde = { version = "1.0.219", features = ["derive"] }
ts-rs = { version = "10.1.0", features = [
  "ordered-float-impl",
  "ordered-float",
  "format",
] }

[dev-dependencies]
serde_json = "1.0.140"

[features]
core = []

