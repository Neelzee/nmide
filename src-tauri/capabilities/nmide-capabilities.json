{
  "identifier": "nmide-capabilities",
  "description": "permissions that were migrated from v1",
  "windows": [
    "main",
    "nmide-*"
  ],
  "remote": {
    "urls": [
      "http://localhost",
      "http://localhost/*",
      "asset://localhost",
      "asset://localhost/*"
    ]
  },
  "permissions": [
    "core:webview:allow-create-webview-window",
    "core:window:allow-set-title",
    "core:default",
    "fs:allow-read-file",
    "fs:allow-read-dir",
    {
      "identifier": "fs:scope",
      "allow": [
        "$APPCONFIG/*",
        "$APPDATA/*",
        "$APPLOG/*",
        "$APPCACHE/*"
      ]
    },
    "shell:allow-open",
    "fs:default",
    "shell:default",
    "log:default",
    "dialog:default"
  ]
}